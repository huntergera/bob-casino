@media all and (-ms-high-contrast: none), (-ms-high-contrast: active)
	.page-header
		font-size 0
		
		.header-content_line-1:after
			background none
			color #fff
		
		.header-content_span-1::after
			background none
			color #f8d856

		.header-content_span-2::after
			background none
			color #fff
	
	.registration 
		.registration-number::after,
		.registration-tagline-2::after
			background none
			color #f8d856
	
	.registration-items>*
		display inline-block
		opacity 1
		
			