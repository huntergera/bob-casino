@media only screen and (max-width: 1420px)
	.container
		width 900px
	
	.page-header
		
		.human-1
			top 99px
			left -240px
			width 52%
			
			img
				width 100%
			
		.header-content
			padding: 20px 0 55px;
		
		.header-content_icon
			left: 45.3%;
			top: 26%;
			
		.logo
			img
				width 77%
		
		.logo_text
			font-size: 14px;
			padding: 4px 0 0;
		
		.header-content_line-1
			font-size: 30px;
			margin: 67px 0 0 0;
		
		.header-content_line-1_divider-1
			top 7px
			width: 27px;
			left: 34.6%;
			height: 3px;
		
		.header-content_line-1_divider-2
			top: 7px;
			width: 27px;
			right: 34.6%;
			height: 3px;
		
		.header-content_line-2
			font-size: 54px;
		
		.header-content_line-3
			font-size: 52px;
		
		.btn_span
			font-size 36px
		
		.header-content_span-2
			font-size: 45px;
			
		.header-content_span-1
			font-size: 90px;
		
		.stone
			width 8%
			
			img
				width 100%
		
		.leaf
			left: 85%;
			width: 248px;
			bottom: -168px;
			
			img
				width 100%
		
		.tablet
			width: 1460px;
			left: 53.5%;
			bottom: 145px;
			
			img
				width 100%
		
		.tablet_text
			right: 370px;
			font-size 32px
			top 32px
			transform: rotate(7deg);
		
		.tablet_text_span-1
			font-size: 51px;
		
		.tablet_text_span-2
			font-size: 23px;
		
		.bg-smoke,
		.bg-smoke-1
			height 230px
			bottom 207px
		
		.upper-layer
			width 100%
			top -100px
			img
				width 100%
	
	.registration 
		.registration-item
			width: 290px;
			height: 220px;
		
		.registration-tagline-1
			font-size: 19px;
		
		.registration-tagline-2
			font-size: 18px;
		
		.registration-text
			font-size 13px
	
	.page-footer
		img
			width 100%
		.payments
			padding: 17px 0;
			
			&:before
				box-shadow: inset 210px 0 125px -50px #271801;
			
			&:after
				box-shadow: inset -210px 0 125px -50px #271801;

		.provider_item
			width 12%
		
		.payment_item
			width 9%
		
		.footer-icon-1
			top 52px
			left 83%
			width 140px
			
			img
				width 100%
		
		.footer-text
			font-size 12px



